  rheumatoid-juvenile-arthritis---primary:
    run: rheumatoid-juvenile-arthritis---primary.cwl
    out:
    - output
    in:
      inputModule:
        id: inputModule
        source: inputModule2
      potentialCases:
        id: potentialCases
        source: read-potential-cases-omop/output
  pauciarticular-juvenile-arthritis---primary:
    run: pauciarticular-juvenile-arthritis---primary.cwl
    out:
    - output
    in:
      inputModule:
        id: inputModule
        source: inputModule3
      potentialCases:
        id: potentialCases
        source: rheumatoid-juvenile-arthritis---primary/output
  other-juvenile-arthritis---primary:
    run: other-juvenile-arthritis---primary.cwl
